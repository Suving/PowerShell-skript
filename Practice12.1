function Get-ServiceCount {
    $runningServices = (Get-Service | Where-Object { $_.Status -eq "Running" }).Count
    $stoppedServices = (Get-Service | Where-Object { $_.Status -eq "Stopped" }).Count

    Write-Host "Jooksvad teenused kokku: $runningServices"
    Write-Host "LÃµpetatud teenused kokku: $stoppedServices"
    Write-Host "Teenused kokku: $($runningServices + $stoppedServices)"
}

Get-ServiceCount

